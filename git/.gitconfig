[user]
	name = AureÃÅlien Bottazzini
	email = aurelien@bottazini.com
[core]
	quotepath = false
	excludesfile = /Users/aurelienbottazini/.gitignore_global
	editor = emacsclient -t -a ''
[push]
	default = upstream
[github]
	user = aurelienbottazzini
	tooken = dafc10e4f7c66a933baba52b1f50bc2e
[alias]
        cm = !git-cm
        di = diff
        co = checkout
        b = branch
        ignore-tracked = update-index --assume-unchanged
	ctags = !.git/hooks/ctags
	glog = log -E -i --grep
	sla = log --oneline --decorate --graph --all
	car = commit --amend --no-edit
	unstage = reset
	uncommit = reset --soft HEAD^
	mup = !git checkout master && git pull && git checkout -
	ureset = !git upstream && git reset --hard (git upstream)
[credential]
	helper = osxkeychain
[difftool]
	prompt = false
[mergetool]
	prompt = false
[init]
	templatedir = ~/.git_template
[merge]
        ff = only
[fetch]
        prune = true