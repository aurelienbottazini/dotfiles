super + b
  jumpapp -c firefox firefox-developer-edition
super + i
  jumpapp -c jetbrains-idea idea
super + t
  jumpapp -c main-terminal launch-main-terminal.sh
super + e
  ~/bin/launch-main-emacs.sh
super + space ; space
  dmenu_run
super + space ; r
  dmenu-yes-no.sh "Do you want to reboot?" "reboot.sh"
super + space ; x
  dmenu-yes-no.sh "Do you want to shutdown?" "shutdown.sh"
super + space ; t
  tdd-org-capture.sh
super + space ; Return
  launch-emacs-org-capture.sh
super + p
  dmenu_run
super + o ; s
  xfce4-settings-manager
super + o ; d
  xfce4-display-settings --minimal
Print
  flameshot gui
super + Print
  peek
super + d
  slack
super + a
  jumpapp anki
super + m
  jumpapp -c cmus launch-music-player.sh
super + r
  jumpapp -c ranger launch-ranger.sh
super + f
  jumpapp thunar
XF86AudioMute
  amixer -q sset Master toggle
XF86AudioLowerVolume
  pactl set-sink-volume 0 -5%
XF86AudioRaiseVolume
  pactl set-sink-volume 0 +5%
XF86MonBrightnessUp
  lux -a 5%
XF86MonBrightnessDown
  lux -s 5%