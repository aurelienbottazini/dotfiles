Config { font = "xft:Gotham HTF Black:size=12:antialias=true"
        , additionalFonts = ["xft:Noto Emoji:size=10", "xft:PragmataPro:size=12"]
        , borderColor = "#939598"
        , border = BottomB
	, borderWidth = 3
        , bgColor = "#dad9d6"
        , fgColor = "black"
        , position = TopW L 100
        , commands = [
	Run Date "%a %b %_d" "theDate" 10
        , Run BatteryP ["BAT0"]
        ["-t", "<acstatus>"
        , "-L", "10", "-H", "80"
        , "-l", "#bc3e44", "-h", "#34855b"
        , "--", "-O", "Charging", "-o", "Battery: <left>%"
        ] 10
        , Run Com "date" ["+%H:%M"] "localtime" 10
        , Run Com "./bin/sanFranciscoTime.sh" [] "sftime" 10
        , Run Com "./bin/getVolume.sh" [] "myvolume" 10
        , Run Com "./bin/dropboxStatus.sh" [] "mydropbox" 10
        , Run Com "./bin/wifiStrengh.sh" ["wlp4s0"] "mywifistrengh" 10
        , Run StdinReader
        ]
        , sepChar = "%"
        , alignSep = "}{"
        , template = " %StdinReader% }{ <action=st ranger ~/Dropbox><fn=2>ÔÖ´</fn> %mydropbox%</action>  <action=st alsamixer><fn=2>ÔíÖ</fn> %myvolume%</action>  <action=wicd-client><fn=2>Ôá´</fn> %mywifistrengh%</action>  <fn=2>Ôëï</fn> <action=firefox --new-window https://www.fastmail.com/calendar/>%theDate%</action> <action=firefox --new-window https://www.worldtimebuddy.com><fn=1>üåç</fn>%localtime% <fn=1>üåé</fn>%sftime%</action> %battery% "
        }
