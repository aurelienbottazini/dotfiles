Config { font = "xft:Gotham HTF Black:size=12:antialias=true"
        , additionalFonts = ["xft:PragmataPro Liga:size=12"]
        , borderColor = "#939598"
        , border = BottomB
	, borderWidth = 3
        , bgColor = "#dad9d6"
        , fgColor = "black"
        , position = TopW L 100
        , commands = [
	Run Date "%a %b %_d" "theDate" 10
        , Run BatteryP ["BAT0"]
        ["-t", "<acstatus>"
        , "-L", "10", "-H", "80"
        , "-l", "#bc3e44", "-h", "#34855b"
        , "--", "-O", "Charging", "-o", "Battery: <left>%"
        ] 10
        , Run Com "date" ["+%H:%M"] "localtime" 10
        , Run Com "./bin/sanFranciscoTime.sh" [] "sftime" 10
        , Run Com "./bin/dropboxStatus.sh" [] "mydropbox" 10
        , Run Com "./bin/wifiStrengh.sh" ["wlp4s0"] "mywifistrengh" 10
        , Run StdinReader
        ]
        , sepChar = "%"
        , alignSep = "}{"
        , template = " %StdinReader% }{ <action=`nautilus ~/Dropbox`><fn=1></fn>%mydropbox%</action> <action=launch-settings.sh sound><fn=1></fn></action>  <action=launch-settings.sh wifi><fn=1></fn> %mywifistrengh%</action>  <fn=1></fn> <action=gsimplecal>%theDate%</action> <action=chromium-browser --new-window --app=https://www.worldtimebuddy.com><fn=1>⌚</fn> %localtime% <fn=1>⌚</fn> %sftime%</action> %battery% "
        }
